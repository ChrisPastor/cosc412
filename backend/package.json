{
  "name": "healthy-competition-backend",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "prod:start": "node ./build/index.js",
    "build": "tsc",
    "test": "tsc && npm run test-unit && npm run test-coverage",
    "test-unit": "mocha",
    "test-coverage": "tsc && nyc mocha"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/mongodb": "^3.5.33",
    "@types/node": "^14.14.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.3",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1",
    "source-map-support": "^0.5.19",
    "supertest": "^6.0.1",
    "ts-node": "^9.0.0"
  },
  "mocha": {
    "exit": true,
    "timeout": 100000,
    "recursive": true,
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register",
      "./setupTests.js"
    ],
    "spec": [
      "./src"
    ]
  },
  "nyc": {
    "include": [
      "./src/**/*.ts"
    ],
    "extends": "@istanbuljs/nyc-config-typescript",
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "instrument": true,
    "sourcemap": false
  }
}
